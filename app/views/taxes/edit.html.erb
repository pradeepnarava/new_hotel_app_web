<style>
  .edit_tax{
    height:400px;
    overflow: scroll;
}
</style>
<%= form_for @tax, {:url=>"/taxes/update"} do |f| %>
  <%= f.hidden_field :id %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %></br>

  <%= f.label :percentage,"Percentage"%>

  <%= f.text_field :percentage %></br>


  <%= f.submit %>
<% end %>