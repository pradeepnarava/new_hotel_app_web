
 <div style="width:800px;height:500px;background-color:white;margin:auto;border:3px solid #333333;overflow:scroll;">
<table class="table table-hover"style="border:1px solid rgb(221,221,221);margin:auto; ">
  <tr style="background-color:#ffcccc">
    <td> S.NO</td> <td>Card No</td><td>Counter</td><td>Transaction Type</td><td>Amount</td><td>Balance</td>
  </tr>
  <%@i=0%>
<%@transactions.each do |s|%>

  <tr>
    <td><%@i+=1%><%=@i%></td><td><%=s.card%></td><td><%=s.counter%></td><td><%=s.transaction_type%></td><td><%=s.cost%></td><td><%=s.balance%></td><td><%#=link_to_modal "edit", {:controller=>'items',:action=>'edit',:id=>s.id},:class=>"btn-small btn-primary"%></td>

  </tr>

<%end%>
</table>


</div>

