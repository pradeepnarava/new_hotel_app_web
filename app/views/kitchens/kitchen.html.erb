<div id="myDiv">
  <div class="page-header" style="margin-top: 10px;">
    <h1 style="text-align:left;color:#81b886;font-variant:small-caps;font-family:fantasy;margin-left:20px;">Kitchens</h1>
     </div>

  <div style="width: 600px;height:800px; background-color:white;margin-bottom: 50px;float: left;margin-left: 30px;overflow: scroll;">
    <table class="table table-bordered">
      <tr style="background-color:blanchedalmond; ">
        
      <div>
        <%= form_tag '/kitchens/show ' %>
        
        <%= select :kitchen, :kitchen_id,@kitchens.collect {|c| [c.name, c.id]}, { :prompt => 'Select Kitchen' }%>
        <%= submit_tag "Go!" %>
      </div>
               
      </tr>
    </table>
  </div>
  </div>



